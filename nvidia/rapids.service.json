 {
      "kind": "Service",
      "apiVersion": "v1",
      "metadata": {
	"name": "rapids"
      },
      "spec": {
	"selector": {
	  "app": "rapids"
	},
	"ports": [
	  {
            "name": "http",
            "protocol": "TCP",
            "port": 8888,
            "targetPort": 8888
	  },
	  {
            "name": "grpc",
            "port": 8787,
            "targetPort": 8787
	  },
	  {
            "name": "prometheus",
            "port": 8786,
            "targetPort": 8786
	  }
	],
	"sessionAffinity": "None",
        "type": "ClusterIP"
      }
    }
